{
  "name": "ember-cli-flash",
  "version": "2.1.3",
  "description": "Simple, highly configurable flash messages for ember-cli",
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint:hbs lint:js test:*",
    "test:all": "ember try:each",
    "contributors": "npx contributor-faces -e \"(*-bot|*\\[bot\\]|*-tomster|homu)\""
  },
  "bugs": "https://github.com/poteto/ember-cli-flash/issues",
  "homepage": "https://github.com/poteto/ember-cli-flash",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "contributors": [
    {
      "name": "Mike North"
    },
    {
      "name": "Scott Batson"
    },
    {
      "name": "Brian Runnells"
    }
  ],
  "dependencies": {
    "@ember/render-modifiers": "^2.0.2",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@types/ember": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.28.5",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "0.2.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "lerna-changelog": "^1.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.65.0"
  },
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "flash messages",
    "flash",
    "growl",
    "toastr",
    "notifications",
    "notify"
  ],
  "repository": "https://github.com/poteto/ember-cli-flash",
  "license": "MIT",
  "author": "Lauren Tan",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "ember": {
    "edition": "octane"
  }
}
